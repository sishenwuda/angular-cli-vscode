{
  "name": "angular-cli-vscode",
  "displayName": "angular-cli-vscode",
  "description": "",
  "version": "0.0.1",
  "publisher": "wimas",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ngGenerate"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.ngGenerate",
        "title": "ngGenerate"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "",
          "command": "extension.ngGenerate",
          "group": "Angular Cli"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/q": "^1.0.0",
    "mocha": "^2.3.3",
    "typescript": "^2.2.0",
    "vscode": "^1.1.0"
  },
  "dependencies": {
    "@angular/cli": "^1.0.6",
    "q": "^1.5.0"
  }
}
